pipeline {
    agent any

    stages {

      stage ('test') 
      { 
        steps {  
             sh '''
             chmod +x test.sh
             ./test.sh
             
             '''
             }
     
      }


     stage('build') {
            steps {
                sh '''
                   docker build -t docker.io/moatazxz/myapp:latest  .
                   docker images
                '''
            }
        }

     




    }

}